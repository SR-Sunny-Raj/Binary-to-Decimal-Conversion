MIN MAX PROGRAM
#include <iostream>
#include<climits>

using namespace std;

int main(){
 int n  , maxi = INT_MIN,mini = INT_MAX ; 
 cin>>n ; 
 int a[n] ;
 for (int i = 0; i < n; i++)
 {
     /* code */
     cin>>a[i] ; 
 }
 for (int i = 0; i < n; i++)
 {
     /* code */
     maxi = max(maxi,a[i]) ; 
     mini = min(mini , a[i]) ; 
 }
 cout<<maxi<<" "<<mini ; 
 

}
